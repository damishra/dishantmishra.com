{
  "ci": {
    "collect": {
      "startServerCommand": "npm run start",
      "startServerReadyPattern": "ready on",
      "url": [
        "https://dishantmishra.com",
        "https://dishantmishra.com/portfolio",
        "https://dishantmishra.com/about",
        "https://dishantmishra.com/contact"
      ],
      "numberOfRuns": 3
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", { "minScore": 0.8 }],
        "first-contentful-paint": ["error", { "maxNumericValue": 2000 }],
        "interactive": ["error", { "maxNumericValue": 5200 }],
        "speed-index": ["error", { "maxNumericValue": 4300 }],
        "resource-summary:script:size": [
          "error",
          { "maxNumericValue": 170000 }
        ],
        "resource-summary:total:size": ["error", { "maxNumericValue": 300000 }]
      }
    },
    "upload": { "target": "temporary-public-storage" }
  }
}
